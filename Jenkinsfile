pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Hello World Cesar'
            }
        }
		stage('Extract') {
            steps {
                echo 'Hello World 2 Augusto'
            }
        }
		stage('Test') {
            steps {
                echo 'Hello World 3 Charalla'
            }
        }
		stage('Deploy') {
            steps {
                echo 'Hello World 4 Olazo'
            }
        }
    }
}